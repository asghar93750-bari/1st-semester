#include <iostream>
using namespace std;

int main(){
      int n{};
      cout<<"Enter a number: ";
      cin>>n;
      if(n<2){
            cout<<"NOT PRIME";
            return 0;
      }
      else{
            for(int i=2; i<n; i++){
                  if(n%i==0){
                        cout<<"NOT PRIME";
                        return 0;
                  }
            }
            cout<<"NUMBER IS PRIME";
      }
      return 0;
}
