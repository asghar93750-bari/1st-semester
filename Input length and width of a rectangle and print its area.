
#include <iostream>
using namespace std;

int main()
{
      int lenght{},width{},area{};
      cout<<"Enter the lenght of rectangle: ";
      cin>>lenght;
      cout<<"Enter the width of rectangle: ";
      cin>>width;
      area=lenght*width;
      cout<<"The area of rectangle: "<<area<<endl;
      return 0;

}
